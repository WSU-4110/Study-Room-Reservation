{
  "buildCommand": "next build",
  "outputDirectory": ".next",
  "framework": "nextjs",
  "rewrites": [
    {
      "source": "/sw.js",
      "destination": "/_next/static/sw.js"
    },
    {
      "source": "/workbox-:hash.js",
      "destination": "/_next/static/workbox-:hash.js"
    }
  ],
  "headers": [
    {
      "source": "/sw.js",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-cache, no-store, must-revalidate"
        }
      ]
    }
  ]
}